{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.stemoctobermagazine.org/api/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://api.stemoctobermagazine.org"
  }
}
